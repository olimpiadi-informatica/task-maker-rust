{
  "name": "task-maker",
  "displayName": "Task Maker",
  "publisher": "Edoardo Morassutto",
  "description": "Visual Studio Code extension for making task-maker tasks.",
  "author": {
    "name": "Edoardo Morassutto",
    "email": "edoardo.morassutto@gmail.com",
    "url": "https://edomora97.github.io"
  },
  "homepage": "https://github.com/edomora97/task-maker-rust",
  "repository": "https://github.com/edomora97/task-maker-rust",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.64.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "main": "./out/extension.js",
  "activationEvents": [
    "onLanguage:gengen"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts"
  },
  "contributes": {
    "languages": [
      {
        "id": "gengen",
        "aliases": [
          "gen/GEN",
          "gengen"
        ],
        "extensions": [
          "GEN"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "gengen",
        "scopeName": "source.gengen",
        "path": "./syntaxes/gengen.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "@types/vscode": "^1.64.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.9.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.1",
    "typescript": "^4.5.5",
    "@vscode/test-electron": "^2.1.2"
  }
}
